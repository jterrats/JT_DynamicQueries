>
    <label>LIKE Patterns Mixed</label>
    <protected>false</protected>
    <values>
        <field>JT_BaseQuery__c</field>
        <value xsi:type="xsd:string">SELECT Id, Name, Website, Phone
FROM Account 
WHERE (Name LIKE :startsWith OR Name LIKE :endsWith)
  AND Website LIKE :websitePattern
  AND Phone LIKE :phonePattern
  AND Description LIKE :descriptionKeyword
ORDER BY Name
LIMIT 50</value>
    </values>
    <values>
        <field>JT_Binding__c</field>
        <value xsi:nil="true" />
    </values>
    <values>
        <field>JT_ObjectName__c</field>
        <value xsi:type="xsd:string">Account</value>
    </values>
</CustomMetadata>
