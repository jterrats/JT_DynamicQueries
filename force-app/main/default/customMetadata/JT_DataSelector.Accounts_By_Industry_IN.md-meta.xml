>
    <label>Accounts By Industry (IN)</label>
    <protected>false</protected>
    <values>
        <field>JT_BaseQuery__c</field>
        <value
      xsi:type="xsd:string"
    >SELECT Id, Name, Industry, AnnualRevenue, Type 
FROM Account 
WHERE Industry IN :industries 
ORDER BY Name</value>
    </values>
    <values>
        <field>JT_Bindings__c</field>
        <value xsi:type="xsd:string">{
  &quot;industries&quot;: {
    &quot;type&quot;: &quot;List&lt;String&gt;&quot;,
    &quot;required&quot;: true,
    &quot;description&quot;: &quot;List of industries to filter by&quot;
  }
}</value>
    </values>
    <values>
        <field>JT_ObjectName__c</field>
        <value xsi:type="xsd:string">Account</value>
    </values>
</CustomMetadata>
